{
  "manifest_version": 3,
  "name": "SendIQ",
  "version": "1.4.0",
  "description": "Send the same email to many recipients quickly â€” each recipient gets an individual message (no BCC) for free.",
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "SendIQ"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "key": "",
  "oauth2": {
    "client_id": "287751131452-u6j9labqsaiqmihdja0dg7f8l9ckh4n4.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.send",
      "https://www.googleapis.com/auth/gmail.modify",
      "openid",
      "email",
      "profile"
    ]
  }
}
